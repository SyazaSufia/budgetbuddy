{
    "version": 2,
    "builds": [
      {
        "src": "src/package.json",
        "use": "@vercel/next"
      },
      {
        "src": "server/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/src/$1"
      }
    ]
  }
  